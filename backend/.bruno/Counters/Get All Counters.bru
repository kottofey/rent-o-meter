meta {
  name: Get All Counters
  type: http
  seq: 2
}

get {
  url: {{BASE_URL}}/counters?includes[]=Bill
  body: none
  auth: inherit
}

params:query {
  includes[]: Bill
  ~scopes[withPeriod][start]: 2025-11-01
  ~scopes[withPeriod][end]: 2026-12-01
  ~scopes[renteeId]: 2
  ~scopes[]: isDebt
  ~includes[]: Rentee
}

settings {
  encodeUrl: true
  timeout: 0
}
